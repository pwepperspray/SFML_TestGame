#include<iostream>
#include<SFML/Graphics.hpp>

int main(){

	sf::RenderWindow window(sf::VideoMode(200,300), "abc");
	sf::Texture tileset;
	tileset.loadFromFile("data\\assets\\tile.png");
        sf::Sprite ground;
	ground.setTexture(tileset);
	ground.setTextureRect(sf::IntRect(0,0,8,8));













	while(window.isOpen()){
		sf::Event e;
		while(window.pollEvent(e))
		{
			if(e.type == sf::Event::Closed){
				window.close();
			}
		}
		window.clear();
		window.draw(ground);
		window.display();
	}
	return 0;
}
